#define c_genOtherMethod(method)
#if(method.name.indexOf("toggle")!=-1)
#@c_genToggleMethod(method.name)
#else
	public void #(method.name)(){
		//TODO 请实现业务逻辑代码	
		renderJsonSuccess();
	}
#end
#end

#define genControllerMethod(method)
	#switch(method.name)
		#case("index")
			#@c_index()
		#case("add")
			#@c_add()
		#case("edit")
			#@c_edit()
		#case("save")
			#@c_save()
		#case("update")
			#@c_update()
		#case("delete")
			#@c_delete()
		#case("deleteByIds")
			#@c_deleteByIds()
		#case("toggleEnable")
			#@c_toggleEnable()
		#case("up")
			#@c_up()
		#case("down")
			#@c_down()
		#case("initRank")
			#@c_initRank()
		#case("tree")
			#@c_tree()
		#case("options")
			#@c_options()
		#default
			#@c_genOtherMethod(method)
	#end
#end