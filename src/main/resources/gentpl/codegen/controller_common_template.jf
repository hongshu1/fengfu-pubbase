#define c_edit()
   /**
	* 编辑
	*/
	public void edit() {#setLocal(fctucName=firstCharToLowerCase(codeGen.modelName))
		#(codeGen.modelName) #(fctucName)=service.findById(get#(getIdType?? 'Long')(0));
		if(#(fctucName) == null){
			renderFail(JBoltMsg.DATA_NOT_EXIST);
			return;
		}
		set("#(fctucName)",#(fctucName));
		render("edit.html");
	}
#end


