#redis 生产环境 配置文件 支持配置N个服务端 每个为一组

#默认-主服务端 名字不要修改
[jbolt_cache]
enable      = false
host        = 39.112.213.32
port        = 6379
password    = jbolt
timeout     = 2000
#database   = 0
#clientName = clientname
#过期时间 单位 秒 -1为不设置过期
expires     = -1

#扩展-电商服务端
[mall]
enable      = false
host        = 39.112.213.33
port        = 6379
password    = jbolt
timeout     = 2000
#database   = 0
#clientName = clientname
#过期时间 单位 秒  -1为不设置过期
expires     = -1